<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
             xmlns:b2="urn:import:org.gwtbootstrap3.extras.datetimepicker.client.ui">


    <ui:with field="res" type="ru.kupryanov.client.resources.AppResources"/>
    <g:HTMLPanel>
        <b:Image
                styleName="imagemain" url="https://ru.inshaker.com/uploads/billboard/promo/55/1570439037-1920%D1%85250.jpg"/>
        <b:Navbar>
            <b:NavbarCollapse>
            <b:NavbarNav>
                <b:AnchorListItem styleName="menu"  ui:field="anchorListItemHome" href="#/">Home</b:AnchorListItem>
                <b:AnchorListItem styleName="menu"  ui:field="anchorListItemHistory" href="/#images">History</b:AnchorListItem>
            </b:NavbarNav>
        </b:NavbarCollapse>
        </b:Navbar>
        <div id="Win">
            <div id="form">
                <form class="form-signin">
                    <b:Button ui:field="GoogleButton"  text="in Google" addStyleNames="btn-primary"></b:Button>
                </form>
            </div>
        </div>
            <b:Button dataTarget="#myModal" dataToggle="MODAL" type="PRIMARY" size="LARGE">Тык сюда</b:Button>
        <b:Modal title="Справка" closable="true" fade="true" dataBackdrop="STATIC" dataKeyboard="true" b:id="Modal">
            <b:ModalBody>
                <g:HTML>Здесь должна быть информация о сайте но её пока нет, но вы держитесь, счастья вам, здоровья!!!</g:HTML>
            </b:ModalBody>
            <b:ModalFooter>
                <b:Button type="DANGER" dataDismiss="MODAL">Close</b:Button>
            </b:ModalFooter>
        </b:Modal>

        <b:Button styleName="buttons" dataTarget="#Modal" dataToggle="MODAL" size="EXTRA_SMALL">Справка</b:Button>
        <b:Lead color="red" text="Я устал на:"/>
        <b:Progress type="STRIPED">
            <b:ProgressBar type="DANGER" percent="90" text="90%"/>
        </b:Progress>
        <b:Panel  styleName="pan">
            <b:PanelHeader >
                <b:Heading size="H3" text="Ваш список дел:"/>
            </b:PanelHeader>
            <b.gwt:CellTable styleName="table" ui:field="cellTable"  bordered="true" condensed="true" striped="true" hover="true"/>
            <b:Pagination ui:field="cellTablePagination"/>
        </b:Panel>
        <g:SimplePanel ui:field="main"/>
        <g:DialogBox ui:field="dialogBox"/>
        <b:Modal title="Добавьте что-то" closable="true" fade="true" dataBackdrop="STATIC" dataKeyboard="true" b:id="myModal" ui:field="myModal">
            <b:ModalBody>
                <b:FormGroup validationState="WARNING">
                    <b:FormLabel>Введите задачу:</b:FormLabel>
                    <g:TextArea styleName="inputsm" ui:field="text"/>
                    <b:HelpBlock text=""/>
                </b:FormGroup>
                <b:FormLabel>Укажите срок:</b:FormLabel>
                <b2:DateTimePicker styleName="date" minView="DAY" format="mm/dd/yyyy hh:mm" ui:field="date" language="RU"/>
            </b:ModalBody>
            <b:ModalFooter>
                <b:Button type="PRIMARY" ui:field="button11" >Добавить</b:Button>
                <b:Button type="DANGER" ui:field="close" dataDismiss="MODAL">Close</b:Button>
            </b:ModalFooter>
        </b:Modal>
    </g:HTMLPanel>

</ui:UiBinder>